{
  "root": true,
  "extends": [
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": 0,
    "no-console": 2,
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": ["error"]
  }
}